<?xml version="1.0" encoding="UTF-8"?>
<Downloader>
  <General>
    <Strings>
      <String name="_other">c:\ALM\wwwroot\App_Data\Documents\Other</String>
      <String name="_userguides">c:\ALM\wwwroot\App_Data\Documents\UserGuides</String>
      <String name="_importsamples">c:\ALM\wwwroot\App_Data\Documents\ImportFileSamples</String>
      <String name="_languagebackups">c:\ALM\wwwroot\App_Data\LanguageBackups</String>
      <String name="_systembackups">C:\ALMBackups\SystemBackups</String>
      <String name="_dbonlybackups">C:\ALMBackups\BackupDBOnly</String>
      <String name="_iloggerlogs">C:\ALMBackups\VRH.iLogger.Logfiles</String>
      <String name="_MvcDownLoaderLog">c:\ALM\wwwroot\App_Data\MvcDownloader\VRH_Logs</String>
      <String name="_ServiceScriptlogs">C:\ALMBackups\ScheduledTasksLogs</String>
      <String name="_uploadsALMpackages">C:\ALMBackups\Uploads</String>
      <String name="_scheduledsavedreports">C:\ALMBackups\ScheduledSavedReports</String>
    </Strings>
    <Log folder="_MvcDownLoaderLog" level="3"/>
    <Defaults>
      <Format columns='1' rowheight="20"/>
      <Order by='date' direction='asc'/>
      <Enable subdirs='true' download='false' open='false' delete='false' upload='false' overwrite='false' rolefiltering='false'/>
	  <Rolemask files='true' directories='false'/>
    </Defaults>
  </General>
  <DownloadDefinitions defaultlcid='en-US'>
    <DownloadDefinition id='ScheduledReports' lcid='hu-HU'>
      <Format title='Ütemezett mentett riportok' columns='0'/>
      <Order by='name' direction='asc'/>
      <Enable upload="true" subdirs='true' download='true' open='false' delete='true' overwrite='?' rolefiltering='true'/>
      <Masks>
		<Mask files="true" directories="false">^.*\.[pP][dD][fF]$</Mask>
		<Mask files="false" directories="true">^.*$</Mask>
      </Masks>
	  <Rolemasks>
        <Rolemask files='false' directories='true'>^2\d{3}[01]\d[0123]\d$</Rolemask>
        <Rolemask files='false' directories='true'>^.*_(?'ROLE'ScheduledReportPackage_.*)$</Rolemask>
	  </Rolemasks>
      <PathToDir>_scheduledsavedreports</PathToDir>
      <Help>Ebben a könyvtárban az ütemezetten mentett riportok találhatók.</Help>
    </DownloadDefinition>
    <DownloadDefinition id='ScheduledReports' lcid='en-US'>
      <Format title='Scheduled saved reports' columns='0'/>
      <Order by='name' direction='asc'/>
      <Enable upload="true" subdirs='true' download='true' open='false' delete='true' overwrite='?' rolefiltering='true'/>
      <Masks>
		<Mask files="true" directories="false">^.*\.[pP][dD][fF]$</Mask>
		<Mask files="false" directories="true">^.*$</Mask>
      </Masks>
	  <Rolemasks>
        <Rolemask files='false' directories='true'>^2\d{3}[01]\d[0123]\d$</Rolemask>
        <Rolemask files='false' directories='true'>^.*_(?'ROLE'ScheduledReportPackage_.*)$</Rolemask>
	  </Rolemasks>
      <PathToDir>_scheduledsavedreports</PathToDir>
      <Help>This directory stores the scheduled and saved reports.</Help>
    </DownloadDefinition>
  </DownloadDefinitions>
</Downloader>